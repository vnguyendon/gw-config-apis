    "Commands": [
        {
            "Type": "Node",
            "Id": "HugoCute",
            "ElementValue": "1",
            "CommandLabel": {
                "en": "So cute!",
                "fr": "Trop mignon !",
                "es": "¡Tan lindo!"
            },
            "Steps": [
                {
                    "Type": "Text",
                    "Label": {
                        "en": "Cute maybe, but Hugo was always a bit of a scaredy-cat, even from a young age",
                        "fr": "Mignon, peut-être, mais Hugo a toujours été un trouillard, même à son plus jeune âge",
                        "es": "Lindo, tal vez, pero Hugo siempre fue un cobarde, incluso de joven"
                    }
                },
                {
                    "Type": "AnimatedGif",
                    "Parameters": {
                        "Source": "Giphy",
                        "Path": "Puc4FZWExJc0E"
                    }
                },
                {
                    "Type": "Pause",
                    "Parameters": {
                        "Mode": "Wait",
                        "ms": 2000
                    }
                },
                {
                    "Type": "Text",
                    "Label": {
                        "en": "And he was not exactly leading a productive life...",
                        "fr": "Et il ne menait pas vraiment une vie productive...",
                        "es": "Y no estaba realmente llevando una vida productiva..."
                    }
                },
                {
                    "Type": "AnimatedGif",
                    "Parameters": {
                        "Source": "Giphy",
                        "Path": "e3NinswgSNdRu"
                    }
                }
            ],
            "LinksTo": {
                "Type": "Node",
                "Id": "HugoCuteLike",
                "ElementValue": "1",
                "Commands": [
                    {
                        "Type": "Node",
